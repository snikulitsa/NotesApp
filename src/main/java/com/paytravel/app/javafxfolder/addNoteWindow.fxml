<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="100.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.paytravel.app.javafxfolder.AddNoteWindowController">
   <children>
      <HBox layoutX="448.0" layoutY="361.0" spacing="10.0"
            AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addHandler" text="Добавить" />
            <Button mnemonicParsing="false" onAction="#cancelHandler" text="Отмена" />
         </children>
      </HBox>
      <TextField layoutX="13.0" layoutY="325.0" prefHeight="25.0" prefWidth="573.0"
                 AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                 fx:id="noteTextField"
      />
      <Label layoutX="14.0" layoutY="14.0" text="Введите текст заметки:"
             AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"
      />
      <HBox layoutX="14.0" layoutY="69.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
         <children>
            <Label text="Дата создания:" />
            <Label fx:id="dateTimeTextField" text="*здесь будет дата*" />
         </children>
      </HBox>
   </children>
</AnchorPane>
